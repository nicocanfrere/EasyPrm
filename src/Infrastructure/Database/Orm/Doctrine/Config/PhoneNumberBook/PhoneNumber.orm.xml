<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="EasyPrm\PhoneNumberBook\PhoneNumber" table="phone_numbers" repository-class="EasyPrm\PhoneNumberBook\Contract\PhoneNumberInterface">
        <id name="identifier" type="identifier">
            <generator strategy="NONE"/>
        </id>
        <field name="number" type="string" column="number" nullable="true"/>
        <field name="label" type="string" column="label" nullable="true"/>
        <field name="ownerGender" type="integer" column="gender" nullable="true"/>
        <field name="ownerFirstName" type="string" column="owner_first_name" nullable="true"/>
        <field name="ownerLastName" type="string" column="owner_last_name" nullable="true"/>
        <field name="createdAt" column="created_at" type="datetime_immutable" nullable="true"/>
        <field name="updatedAt" column="updated_at" type="datetime" nullable="true"/>
        <many-to-one field="phoneNumberBook" target-entity="EasyPrm\PhoneNumberBook\PhoneNumberBook" inversed-by="numbers">
            <join-column name="phone_number_book_id" referenced-column-name="identifier"/>
        </many-to-one>
    </entity>
</doctrine-mapping>
