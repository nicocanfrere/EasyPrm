<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="EasyPrm\Partner\PartnerAccount" table="partner_partner_account" repository-class="EasyPrm\Partner\Contract\PartnerAccountRepositoryInterface">
        <id name="identifier" type="identifier">
            <generator strategy="NONE"/>
        </id>
        <field name="accountNumber" type="string" column="account_number" nullable="true"/>
        <field name="label" type="string" column="label" nullable="true"/>
        <field name="alias" type="string" column="alias" nullable="true"/>
        <field name="companyNumber" type="string" column="company_number" nullable="true"/>
        <field name="createdAt" column="created_at" type="datetime_immutable" nullable="true"/>
        <field name="updatedAt" column="updated_at" type="datetime" nullable="true"/>
        <one-to-one field="addressBook" target-entity="EasyPrm\AddressBook\AddressBook">
            <join-column name="address_book_identifier" referenced-column-name="identifier" on-delete="SET NULL" nullable="true"/>
        </one-to-one>
        <one-to-one field="phoneBook" target-entity="EasyPrm\PhoneNumberBook\PhoneNumberBook">
            <join-column name="phone_book_identifier" referenced-column-name="identifier" on-delete="SET NULL" nullable="true"/>
        </one-to-one>
    </entity>
</doctrine-mapping>
